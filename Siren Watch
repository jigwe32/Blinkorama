input.onGesture(Gesture.Shake, function () {
    music.siren.play()
    for (let i = 0; i < 4; i++) {
        light.showRing(
        `blue blue blue blue blue blue blue blue blue blue`
        )
        light.showRing(
        "red red red red red red red red red red"
        )
    }
    light.clear()
})
forever(function () {
	
})
